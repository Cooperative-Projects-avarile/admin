#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules/vite/bin/node_modules:/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules/vite/node_modules:/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules:/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules/vite/bin/node_modules:/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules/vite/node_modules:/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules:/Users/johnlee/workSpace/tasks/wang/admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@5.2.8_@types+node@20.12.5_sass@1.74.1/node_modules/vite/bin/vite.js" "$@"
fi
